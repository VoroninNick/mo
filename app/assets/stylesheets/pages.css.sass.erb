.special_offers_wrap
    .special_offers_header_wrap
        border-bottom: 1px dashed #d1d3d4
    ul.special_offers_nav_tabs
        margin: 0
        text-align: center
        padding:
            top: 20px
            bottom: 20px
        li
            list-style: none
            display: inline-block
            label
                color: #a7a9ac !important
                font:
                    size: 1.125rem !important
                    family: 'Playfair Display', serif !important
                    style: italic !important
                line-height: 1.3125rem !important
                padding:
                    left: 20px
                    right: 20px
            &.sets_tab
                border:
                    left: 1px solid #d1d3d4
                    right: 1px solid #d1d3d4
    .special_offers_tab_wrap
        display: none
        padding-bottom: 50px
        .pd_product_pack_wrap
            h3
                margin-bottom: 20px

input.sale_tab_input_status[type=radio]:checked ~ .sf_container
    ul.special_offers_nav_tabs
        li.sale_tab
            label
                color: #6d6e71 !important
    .sale_tab_wrap
        display: block

input.sets_tab_input_status[type=radio]:checked ~ .sf_container
    ul.special_offers_nav_tabs
        li.sets_tab
            label
                color: #6d6e71 !important
    .sets_tab_wrap
        display: block

input.all_special_offers_tab_input_status[type=radio]:checked ~ .sf_container
    ul.special_offers_nav_tabs
        li.all_special_offers_tab
            label
                color: #6d6e71 !important
    .all_special_offers_tab_wrap
        display: block


.symbol_plus_wrap
    width: 12.5%
    float: left
    height: 220px
    position: relative
    .symbol_plus_inner
        width: 40px
        height: 40px
        +my_center_align
            position: absolute
        .symbol_plus
            width: 100%
            height: 100%
            position: relative
            &:before
                display: block
                content: ''
                width: 100%
                height: 5px
                background: #a78c30
                +my_vertical_align
                    position: absolute
            &:after
                display: block
                content: ''
                width: 5px
                height: 100%
                background: #a78c30
                +my_horizontal_align
                    position: absolute
.products_pack_items_wrap
    margin-bottom: 20px
    .bi, .symbol_plus_wrap
        margin-top: 30px
.symbol_equal_wrap
    width: 100%
    .symbol_equal_inner
        width: 40px
        height: 40px
        position: relative
        margin: 0 auto
        .symbol_equal
            width: 100%
            height: 100%
            &:before
                display: block
                content: ''
                width: 100%
                height: 5px
                background: #a78c30
                +my_vertical_align
                    top: 33.3%
                    position: absolute
            &:after
                display: block
                content: ''
                width: 100%
                height: 5px
                background: #a78c30
                +my_vertical_align
                    top: 66.6%
                    position: absolute


