.symbol_plus_wrap
    width: 12.5%
    float: left
    height: 220px
    position: relative
    .symbol_plus_inner
        width: 40px
        height: 40px
        +my_center_align
            position: absolute
        .symbol_plus
            width: 100%
            height: 100%
            position: relative
            &:before
                display: block
                content: ''
                width: 100%
                height: 3px
                background: #c8ae4b
                +my_vertical_align
                    position: absolute
            &:after
                display: block
                content: ''
                width: 3px
                height: 100%
                background: #c8ae4b
                +my_horizontal_align
                    position: absolute
.products_pack_items_wrap
    margin-bottom: 20px
    .bi, .symbol_plus_wrap
        margin-top: 30px
.symbol_equal_wrap
    width: 100%
    .symbol_equal_inner
        width: 40px
        height: 40px
        position: relative
        margin: 0 auto
        .symbol_equal
            width: 100%
            height: 100%
            &:before
                display: block
                content: ''
                width: 100%
                height: 3px
                background: #c8ae4b
                +my_vertical_align
                    top: 33.3%
                    position: absolute
            &:after
                display: block
                content: ''
                width: 100%
                height: 3px
                background: #c8ae4b
                +my_vertical_align
                    top: 66.6%
                    position: absolute


