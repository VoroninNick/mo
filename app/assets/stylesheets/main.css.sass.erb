.delete-item
    width: 20px
    height: 20px
    position: relative
    right: -4px
    cursor: pointer
    &:before, &:after
        display: block
        content: ''
        width: 15px
        height: 1px
        background: #a7a9ac
        position: absolute
        top: 50%
        left: 50%
    &:before
        -webkit-transform: translate(-50%, -50%) rotate(45deg)
        -moz-transform: translate(-50%, -50%) rotate(45deg)
        -ms-transform: translate(-50%, -50%) rotate(45deg)
        -o-transform: translate(-50%, -50%) rotate(45deg)
        transform: translate(-50%, -50%) rotate(45deg)
    &:after
        -webkit-transform: translate(-50%, -50%) rotate(-45deg)
        -moz-transform: translate(-50%, -50%) rotate(-45deg)
        -ms-transform: translate(-50%, -50%) rotate(-45deg)
        -o-transform: translate(-50%, -50%) rotate(-45deg)
        transform: translate(-50%, -50%) rotate(-45deg)
    &:hover
        &:before
            background: #E05858
        &:after
            background: #E05858
