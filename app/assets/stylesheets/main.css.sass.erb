.main_menu_wrap
    width: 100%
    min-height: 100%
    height: auto
    background-color: #e8d69b
    background-image: url('<%=asset_path("mm_backgroundPattern12.png")%>')
    background-size: 128px 128px
    position: relative
    z-index: 9
    .first_menu
        padding:
            top: 100px
        margin-bottom: 30px
        ul
            li
                list-style: none
                position: relative
                a
                    display: block
                    width: 100%
                    text:
                        align: center
                        transform: uppercase
                    color: #dd7272
                    font:
                        size: 1.5rem
                        family: 'Fira Sans', sans-serif
                    line-height: 46px
                    &:hover
                        span
                            position: relative
                            &:before
                                display: block
                                content: ''
                                width: 50px
                                height: 1px
                                background: #dd7272
                                position: absolute
                                top: 50%
                                left: -60px
                            &:after
                                display: block
                                content: ''
                                width: 50px
                                height: 1px
                                background: #dd7272
                                position: absolute
                                top: 50%
                                right: -60px
                    &.title
                        color: #a78c30
                        cursor: default
                        &:hover
                            span
                                &:before
                                    display: none
                                &:after
                                    display: none

    &.open_menu
        -webkit-transform: translateY(-100%)
        -ms-transform: translateY(-100%)
        transform: translateY(-100%)
    .second_menu
        position: relative
        overflow: hidden
        width: 100%
        margin-bottom: 100px
        &:before, &:after
            display: block
            content: ''
            white-space: nowrap
            position: absolute
            left: 0
            color: #a78c30
            background-image: linear-gradient(to right, #a78c30 60%, rgba(255,255,255,0) 0%)
            background-position: bottom
            background-size: 22px 1px
            background-repeat: repeat-x
            width: 100%
            height: 1px
        &:before
            top: 0
        &:after
            bottom: 0
        ul.sm_links
            margin: 0
            margin:
                top: 36px
                bottom: 30px
            text-align: center
            li
                display: inline-block
                a
                    font:
                        style: italic
                        size: 1.12500111rem
                        family: 'Playfair Display', serif
                    color: #dd7272
                    margin: 0 17px
                    position: relative
                    &:before
                        display: block
                        content: '.'
                        position: absolute
                        top: -100%
                        left: -17px
                        font-size: 32px
                    &:hover
                        border-bottom: 1px solid #dd7272
                &:first-of-type
                    a:before
                        display: none
        ul.social_networks_wrap
            margin: 0
            margin-bottom: 30px
            text-align: center
            li
                display: inline-block
                a
                    svg
                        width: 30px
                        height: 30px
                        margin:
                            left: 7px
                            right: 7px
                        path
                            fill: #a78c30
                    &:hover
                        path
                            fill: #dd7272

